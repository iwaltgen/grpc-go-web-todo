  "repository": {
    "type": "git",
    "url": "https://github.com/iwaltgen/grpc-go-web-todo.git"
  },
  "keywords": [
    "svelte",
    "grpc-web",
    "todomvc"
  ],
  "author": {
    "name": "iwaltgen",
    "email": "iwaltgen@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/iwaltgen/grpc-go-web-todo/issues"
  },
  "homepage": "https://github.com/iwaltgen/grpc-go-web-todo#readme",
  "devDependencies": {
    "@sveltejs/kit": "next",
    "@types/cookie": "^0.4.0",
    "@types/google-protobuf": "^3.15.5",
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-svelte3": "^3.2.0",
    "prettier": "2.3.2",
    "prettier-plugin-svelte": "^2.3.1",
    "svelte": "^3.42.1",
    "svelte-check": "^2.0.0",
    "svelte-preprocess": "^4.7.4",
    "tslib": "^2.3.1",
    "typescript": "^4.0.0"
  },
  "type": "module",
  "dependencies": {
    "google-protobuf": "^3.17.3",
    "grpc-web": "^1.2.1",
    "rxjs": "^7.3.0"
