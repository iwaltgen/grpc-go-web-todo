  "repository": {
    "type": "git",
    "url": "https://github.com/iwaltgen/grpc-go-web-todo.git"
  },
  "keywords": [
    "svelte",
    "grpc-web",
    "todomvc"
  ],
  "author": {
    "name": "iwaltgen",
    "email": "iwaltgen@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/iwaltgen/grpc-go-web-todo/issues"
  },
  "homepage": "https://github.com/iwaltgen/grpc-go-web-todo#readme",
  "devDependencies": {
    "@sveltejs/kit": "next",
    "@types/cookie": "^0.4.0",
    "@types/google-protobuf": "^3.15.5",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-svelte3": "^3.2.1",
    "prettier": "2.4.1",
